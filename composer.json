{
    "name": "intermaterium/cassandra-native",
    "description": "A native Apache Cassandra and ScyllaDB connector for PHP based on the CQL binary protocol with support for persistent connections",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "cassandra",
        "database",
        "cql"
    ],
    "require": {
        "php": ">=8.0"
    },
    "suggest": {
        "ext-lz4": "Required for enabling compression between Cassandra"
    },
    "autoload": {
        "psr-4": {
            "CassandraNative\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Uri Hartmann",
            "homepage": "https://www.humancodes.org/projects/php-cql"
        },
        {
            "name": "Christopher Birmingham",
            "email": "chris.birmingham@hotmail.co.uk"
        }
    ]
}
